        <referenceBlock name="company_profile">
            <block class="Magento\CompanyShipping\Block\Company\Profile\ShippingMethod" name="company.shipping.methods"
                   after="-"
                   ifconfig="btob/website_configuration/company_active"
                   template="Magento_CompanyShipping::company/management/profile/shipping-method-list.phtml" />
        </referenceBlock>
        <referenceBlock name="company_shipping_method" display="false" />
    </body>
</page>
