<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer-account-navigation-wish-list-link">
            <arguments>
                <argument name="label" xsi:type="string" translate="true">Projects</argument>
            </arguments>
        </referenceBlock>

        <referenceBlock name="customer-account-navigation-account-edit-link">
            <arguments>
                <argument name="label" xsi:type="string" translate="true">My Account Information</argument>
            </arguments>
        </referenceBlock>

        <referenceBlock name="customer-account-navigation-billing-agreements-link">
            <arguments>
                <argument name="label" xsi:type="string" translate="true">My Billing Agreements</argument>
            </arguments>
        </referenceBlock>

        <referenceBlock name="customer-account-navigation-company-profile-link">
            <arguments>
                <argument name="label" xsi:type="string" translate="true">My Company Profile</argument>
            </arguments>
        </referenceBlock>

        <referenceBlock name="customer-account-navigation-company-users-link">
            <arguments>
                <argument name="label" xsi:type="string" translate="true">My Company Users</argument>
            </arguments>
        </referenceBlock>

        <referenceBlock name="customer-account-navigation-company-documents-link">
            <arguments>
                <argument name="label" xsi:type="string" translate="true">My Trade Documents</argument>
            </arguments>
        </referenceBlock>

        <referenceBlock name="customer-account-navigation-address-link">
            <arguments>
                <argument name="label" xsi:type="string" translate="true">My Address Book</argument>
            </arguments>
        </referenceBlock>

        <move element="page.main.title" destination="columns.top" before="-" />

        <referenceBlock name="catalog.compare.sidebar" remove="true" />
        <referenceBlock name="customer-account-navigation-checkout-sku-link" remove="true" />
        <referenceBlock name="customer-account-navigation-downloadable-products-link" remove="true" />
        <referenceBlock name="customer-account-navigation-reward-link" remove="true" />
        <referenceBlock name="customer-account-navigation-giftregistry-link" remove="true" />
        <referenceBlock name="customer-account-navigation-product-reviews-link" remove="true" />
        <referenceBlock name="customer-account-navigation-magento-invitation-link" remove="true" />
        <referenceBlock name="customer-account-navigation-customer-balance-link" remove="true" />
        <referenceBlock name="customer-account-navigation-gift-card-link" remove="true" />
        <referenceBlock name="customer-account-navigation-company-link-cap" remove="true"/>
        <referenceBlock name="customer-account-navigation-newsletter-subscriptions-link" remove="true"/>
        <referenceBlock name="customer-account-navigation-company-roles-link" remove="true"/>
        <referenceBlock name="customer-account-navigation-delimiter-1" remove="true"/>
        <referenceBlock name="customer-account-navigation-delimiter-2" remove="true"/>
        <referenceBlock name="customer-account-navigation-delimiter-b2b-cap" remove="true"/>

    </body>
</page>
