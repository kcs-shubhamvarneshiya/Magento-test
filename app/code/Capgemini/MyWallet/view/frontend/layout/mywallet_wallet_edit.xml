<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Edit Wallet</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Capgemini\MyWallet\Block\Customer\Index" name="my_wallet_wallet_edit" template="Capgemini_MyWallet::customer/walletEdit.phtml" cacheable="false">
            </block>
        </referenceContainer>
    </body>
</page>
